{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/components/utils/*"],
      "@assets/*": ["src/assets/*"],
      "@images/*": ["src/assets/images/*"],
      "@layouts/*": ["src/layouts/*"],
      "@scss/*": ["src/scss/*"],
      "@js/*": ["src/scripts/*"]
    }
  }
}